<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_stock_quant_tree_inherit" model="ir.ui.view">
        <field name="name">stock.quant.tree.inherit.label</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='quantity']" position="after">
                <button name="action_print_label"
                        string="Print Labels"
                        type="object"
                        class="btn-secondary"
                        icon="fa-print"/>
            </xpath>

            <xpath expr="//header/button[@name='action_inventory_at_date']" position="before">
                <button name="action_print_label"
                        string="Print labels"
                        type="object"
                        class="btn-secondary"
                        icon="fa-print"
                        display="always"/>
            </xpath>
        </field>
    </record>
</odoo>
